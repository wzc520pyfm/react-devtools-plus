{
  "name": "@react-devtools-plus/scan",
  "type": "module",
  "version": "0.3.2",
  "description": "React Scan integration for React DevTools",
  "keywords": [
    "react",
    "react-devtools-plus",
    "react-scan",
    "performance",
    "profiler"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "stub": "tsup --watch",
    "prepare:type": "tsc --declaration --emitDeclarationOnly --outDir dist"
  },
  "peerDependencies": {
    "@react-devtools-plus/core": "workspace:*",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@react-devtools-plus/core": {
      "optional": true
    }
  },
  "dependencies": {
    "react-scan": "^0.4.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.72",
    "@types/react-dom": "^18.2.23",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  }
}
