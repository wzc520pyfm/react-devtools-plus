{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "@react-devtools-plus/shared#stub": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "@react-devtools-plus/kit#stub": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@react-devtools-plus/shared#prepare:type"
      ]
    },
    "@react-devtools-plus/core#stub": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@react-devtools-plus/shared#prepare:type"
      ]
    },
    "@react-devtools-plus/client#build": {
      "dependsOn": [
        "@react-devtools-plus/kit#build",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@react-devtools-plus/client#stub": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@react-devtools-plus/kit#prepare:type"
      ]
    },
    "@react-devtools-plus/overlay#build": {
      "dependsOn": [
        "@react-devtools-plus/kit#build",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@react-devtools-plus/overlay#stub": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@react-devtools-plus/kit#prepare:type"
      ]
    },
    "react-devtools-plus#build": {
      "dependsOn": [
        "@react-devtools-plus/client#build",
        "^build"
      ],
      "outputs": [
        "dist/**",
        "client/**"
      ],
      "cache": false
    },
    "react-devtools-plus#stub": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@react-devtools-plus/scan#prepare:type",
        "@react-devtools-plus/overlay#build",
        "@react-devtools-plus/client#build"
      ]
    },
    "@react-devtools-plus/scan#stub": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "@react-devtools-plus/core#prepare:type"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*.?{c|m}js",
        "dist/**/*.d.ts"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "prepare:type": {
      "dependsOn": [
        "^prepare:type"
      ],
      "outputs": [
        "dist/**/*.d.ts"
      ]
    },
    "stub": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^prepare:type"
      ]
    }
  },
  "ui": "stream"
}
